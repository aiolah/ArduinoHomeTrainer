    {
        "id": "015cf42f4db92162",
        "type": "function",
        "z": "37d68567eea3b8a7",
        "name": "Update progress bar",
        "func": "// Float\nlet pourcentageProgressBar = flow.get(\"pourcentageProgressBar\");\nlet currentPourcentage = flow.get(\"currentPourcentage\");\n\n// Float\nlet potentialMaxReached = flow.get(\"potentialMaxReached\");\npotentialMaxReached = parseInt(currentPourcentage + pourcentageProgressBar);\nlet updatingBar = flow.get(\"updatingBar\");\n\nconsole.log(\"pourcentageProgressBar : \" + pourcentageProgressBar);\nconsole.log(\"potentialMaxReached\", potentialMaxReached);\n\nif(!updatingBar && potentialMaxReached == 0)\n{\n    msg.payload = 0;\n    return msg;\n}\nif(potentialMaxReached <= 100 && updatingBar)\n{\n    console.log(\"updateBar !!!\");\n    currentPourcentage = parseInt(currentPourcentage + pourcentageProgressBar);\n    console.log(\"currentPourcentage : \" + currentPourcentage);\n    // On update la progress bar\n    msg.payload = currentPourcentage;\n\n    flow.set(\"currentPourcentage\", currentPourcentage);\n\n    return msg;\n}\n// Si la barre ne vaut pas 100% et qu'on doit encore l'update\nelse if(potentialMaxReached > 100 && updatingBar)\n{\n    flow.set(\"updatingBar\", false);\n    msg.payload = 100;\n    return msg;\n}",
